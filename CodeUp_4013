//진법 변환

import java.util.Scanner;

public class Main{
	static int n;
	public static void main(String[] args)
	{
		Scanner scan=new Scanner(System.in);
		
		int nn=scan.nextInt();
		
		System.out.println(2+" "+Integer.toBinaryString(nn));
		System.out.println(8+" "+Integer.toOctalString(nn));
		System.out.println(16+" "+(Integer.toHexString(nn)).toUpperCase());
		
		
		
	}
	
}
