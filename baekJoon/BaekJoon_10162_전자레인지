
import java.util.Scanner;

public class Main{
	public static void main(String[] args)
	{
		Scanner scan=new Scanner(System.in);
		int[] ar= {300,60,10};
		int a=0;
		int n=scan.nextInt();
		
		for(int i=0;i<ar.length;i++)
		{
			a=0;
			while(n>=ar[i]) {
				n-=ar[i];
				a++;
			}
			ar[i]=a;
		}
	
		if(n==0) System.out.println(ar[0]+" "+ar[1]+" "+ar[2]);
		else System.out.println(-1);
	}
}
