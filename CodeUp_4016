//세 수의 최대공약수 구하기


import java.util.Scanner;
public class Main{
	static int n;
	public static void main(String[] args)
	{
		Scanner scan=new Scanner(System.in);
		
		int a=scan.nextInt();
		int b=scan.nextInt();
		int c=scan.nextInt();
		
		for(int i=a;i>0;i--)
		{
			if(a%i==0&&b%i==0&&c%i==0) {
				System.out.println(i);
				break;
				}
		}
		
		
		
		
	}
	
}
