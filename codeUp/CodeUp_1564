//함수로 최대공약수 찍기

import java.util.Scanner;

public class Main{
	public static void main(String[] args)
	{
		Scanner scan=new Scanner(System.in);
		int a=scan.nextInt();
		int b=scan.nextInt();
		
		Gcd g=new Gcd();
		System.out.println(g.gcd(a,b));
		
		
	}
}
class Gcd{
	public int gcd(int n, int m) {
		int maxGcd=0;
		for(int i=1;i<=n;i++)
		{
			if(n%i==0 && m%i==0) maxGcd=i;
		}
		
		return maxGcd;
	}
}
