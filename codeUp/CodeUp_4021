//홀수의 합 구하기

import java.util.Scanner;

public class Main{
	public static void main(String[] args)
	{
		Scanner scan=new Scanner(System.in);
		
		int[] ar=new int[7];
		int n=0;
		for(int i=0;i<7;i++)
		{
			ar[i]=scan.nextInt();
			if(ar[i]%2==1) n+=ar[i];
		}
		
		if(n==0) System.out.println(-1);
		else System.out.println(n);
	}
}
